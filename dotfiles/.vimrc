filetype indent on
filetype on
filetype plugin on

set expandtab
set nocompatible
set number
set ruler
set shiftwidth=4
set spell
set spelllang=en
set t_te=^[[?47l    " on exit, pop terminal state from alternate screen
set t_ti=^[[?47h    " on init, push terminal state to alternate screen
set tabstop=4
set term=xterm-256color

syntax on
